<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="de.vogella.android.wallpaper" android:versionCode="1"
	android:versionName="1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">


		<service android:name="MyWallpaperService" android:permission="android.permission.BIND_WALLPAPER"
			android:label="Wallpaper Example ">
			<intent-filter>
				<action android:name="android.service.wallpaper.WallpaperService"></action>
			</intent-filter>
			<meta-data android:name="android.service.wallpaper"
				android:resource="@xml/mywallpaperdescription"></meta-data>
		</service>
		<activity android:name=".MyPreferencesActivity"></activity>
	</application>
	<uses-sdk android:minSdkVersion="10" />
	<uses-feature android:name="android.software.live_wallpaper"
		android:required="true"></uses-feature>

</manifest> 